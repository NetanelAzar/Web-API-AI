<h1>My AI</h1>
<div class="container">
  {{#each text}}
    <section class="card">
      <div class="card-header">
        <h2>You : {{this.prompt}}</h2>
      </div>
      <div class="card-body">
        <p class="answer"><h2>Answer :</h2> {{this.answer}}</p>

      </div>
    </section>
  {{/each}}
</div>
<div class="container">
  <div class="card-input">
    <input type="text" id="prompt" placeholder="הזן את ההודעה שלך ל-gemini" />
    <button class="card-button" onclick="#">שלח</button>
  </div>
</div>