<div class="content">
    <div class="cards">
        <div class="card">
            <div class="box">
                <h1>{{users.length}}</h1> <!-- מספר המשתמשים -->
                <h3>Users</h3> <!-- כותרת "משתמשים" -->
            </div>
            <div class="icon-case">
                <img src="students.png" alt=""> <!-- תמונת סמל משתמשים -->
            </div>
        </div>
        <div class="card">
            <div class="box">
                <h1>{{text.length}}</h1> <!-- מספר הטקסטים -->
                <h3>Gemini creation</h3> <!-- כותרת "יצירת ג'מיני" -->
            </div>
            <div class="icon-case">
                <img src="teachers.png" alt=""> <!-- תמונת סמל יצירת ג'מיני -->
            </div>
        </div>
        <div class="card">
            <div class="box">
                <h1>{{url.length}}</h1> <!-- מספר ה-URL -->
                <h3>URL</h3> <!-- כותרת "URL" -->
            </div>
            <div class="icon-case">
                <img src="schools.png" alt=""> <!-- תמונת סמל URL -->
            </div>
        </div>

      <div class="card">
    <div class="box">
        <h1>{{ sessionCount }}</h1> <!-- מספר המשתמשים המחוברים -->
        <h3>משתמשים מחוברים</h3> <!-- כותרת "משתמשים מחוברים" -->
    </div>
    <div class="icon-case">
        <img src="income.png" alt=""> <!-- תמונת סמל המשתמשים המחוברים -->
    </div>
</div>

     <div class="card">
            <div class="box">
                <h1>{{QRcode.length}}</h1> <!-- מספר הטקסטים -->
                <h3>QR</h3> <!-- כותרת "יצירת ג'מיני" -->
            </div>
            <div class="icon-case">
                <img src="teachers.png" alt=""> <!-- תמונת סמל יצירת ג'מיני -->
            </div>
        </div>

    <div class="content-2">
        <div class="recent-payments">
            <div class="title">
                <h2>Users</h2> <!-- כותרת "משתמשים" -->
                <a href="/admin/userAdmin" class="btn">View All</a> <!-- קישור לצפייה בכל המשתמשים -->
            </div>

            {{#each users}} <!-- הצגת כל המשתמשים בטבלה -->
            <table>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>phone</th>
                    <th>isAdmin</th>
                    
                </tr>
                <tr>
                    <td>{{this.fullName}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.phone}}</td>
                    <td>{{this.isAdmin}}</td>
                    
                </tr>
            </table>
            {{/each}}

        </div>

        <div class="recent-payments">
            <div class="title">
                <h2>Administrators</h2> <!-- כותרת "מנהלים" -->
                <a href="#" class="btn">View All</a> <!-- קישור לצפייה בכל המנהלים -->
            </div>
            <table>
                <tr>
                    <th>Profile</th>
                    <th>Name</th>
                    <th>Title</th>
                </tr>
                <tr>
                    <td><img src="#" alt=""></td>
                    <td>Neanel Azar</td>
                    <td>Website developer</td>
                </tr>
                
            </table>
        </div>
    </div>
</div>
